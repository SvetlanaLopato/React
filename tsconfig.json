{
    "compilerOptions": {
        "outDir": "public",
        "target": "es5",
        "module": "es6",
        "moduleResolution": "node",
        "lib": [ "es2015", "dom" ],
        "noImplicitAny": true,
        "jsx": "react",
        "allowSyntheticDefaultImports": true,
        "suppressImplicitAnyIndexErrors": true,
        "sourceMap": true,
        "allowJs": true
    }
}
